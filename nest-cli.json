{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": [
      {
        "include": "src/modules/subscriptions/templates/**/*",
        "outDir": "dist/modules/subscriptions/templates"
      }
    ],
    "watchAssets": true,
    "deleteOutDir": true
  }
}
